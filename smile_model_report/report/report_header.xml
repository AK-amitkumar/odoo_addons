<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="ir_header_img_smile_logo" model="ir.header_img">
            <field name="name">smile_logo</field>
            <field name="type">png</field>
            <field name="img" eval="&quot;&quot;&quot;&quot;&quot;&quot;"/>
        </record>

        <record id="ir_header_webkit_smile" model="ir.header_webkit">
            <field name="name">Smile</field>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">0.0</field>
            <field name="margin_bottom">16.0</field>
            <field name="css" ></field>
            <field name="html"><![CDATA[<html><body></body></html>]]></field>
            <field name="footer_html"><![CDATA[<html>
    <head>
        <meta content="text/html; charset=UTF-8" http-equiv="content-type"/>
        <script>
            function subst() {
            var vars={};
            var x=document.location.search.substring(1).split('&');
            for(var i in x) {var z=x[i].split('=',2);vars[z[0]] = unescape(z[1]);}
            var x=['frompage','topage','page','webpage','section','subsection','subsubsection'];
            for(var i in x) {
            var y = document.getElementsByClassName(x[i]);
            for(var j=0; j<y.length; ++j) y[j].textContent = vars[x[i]];
                }
            }
        </script>
    </head>
    <body onload="subst()">
        <table cellpadding="0" style="border-top: 1px solid #F37021; width: 100%; border-collapse:collapse;">
            <tr>
                <td width="15%" bgcolor="#F37021" style="text-align:center; font-size:12; color:white;"><span class="page"></span> / <span class="topage"></span></td>
                <td width="70%" style="text-align:left; font-size:12; color:#F37021; padding-left: 15px;">&copy; Smile - Open Source Solutions</td>
                <td width="15%" style="text-align:right;">${helper.embed_logo_by_name('smile_logo')|n}</td>
            </tr>
        </table>
    </body>
</html>]]></field>
        </record>

    </data>
</openerp>
