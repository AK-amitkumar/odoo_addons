<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

        <record id="view_publication_number_deletion_wizard_form" model="ir.ui.view">
            <field name="name">publication.number.deletion_wizard.form</field>
            <field name="model">publication.number.deletion_wizard</field>
            <field name="arch" type="xml">
                <form string="Publication Number Deletion" version="7.0">
                    <sheet>
                        <label for="shift_numbers"/>
                        <field name="shift_numbers" class="oe_inline"/>
                    </sheet>
					<footer>
						<button name="button_validate" type='object' string="Validate" class="oe_highlight"/>
						or
						<button string="Cancel" class="oe_link" special="cancel"/>
					</footer>
				</form>
			</field>
		</record>

    </data>
</openerp>
