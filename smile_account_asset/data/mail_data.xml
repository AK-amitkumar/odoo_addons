<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <!-- Asset-related subtypes for messaging / Chatter -->
        <record id="mt_asset_confirmation" model="mail.message.subtype">
            <field name="name">Asset acquised or in progress</field>
            <field name="res_model">account.asset.asset</field>
            <field name="default" eval="False"/>
            <field name="description">Asset acquised or in progress</field>
        </record>
        <record id="mt_asset_validation" model="mail.message.subtype">
            <field name="name">Asset into service</field>
            <field name="res_model">account.asset.asset</field>
            <field name="default" eval="False"/>
            <field name="description">Asset into service</field>
        </record>
        <record id="mt_asset_sale" model="mail.message.subtype">
            <field name="name">Asset sold or scrapped</field>
            <field name="res_model">account.asset.asset</field>
            <field name="default" eval="False"/>
            <field name="description">Asset sold or scrapped</field>
        </record>

    </data>
</openerp>
