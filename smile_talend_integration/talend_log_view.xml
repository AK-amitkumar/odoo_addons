<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

		<!-- Talend Log -->

        <record id="view_talend_log_form" model="ir.ui.view">
            <field name="name">talend.log.form</field>
            <field name="model">talend.log</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Talend Log">
                    <field name="moment" />
                    <field name="pid" />
                    <field name="root_pid" />
                    <field name="father_pid" />
                    <field name="project" />
                    <field name="job" />
                    <field name="context" />
                    <field name="priority" />
                    <field name="type" />
                    <field name="origin" />
                    <field name="message" />
                    <field name="code" />
               </form>
            </field>
        </record> 

  		<record id="view_talend_log_tree" model="ir.ui.view">
            <field name="name">talend.log.tree</field>
            <field name="model">talend.log</field>
            <field name="type">tree</field>
            <field name="priority">2</field>
            <field name="arch" type="xml">
				<tree string="Talend Logs">                				
                    <field name="moment" />
                    <field name="pid" />
                    <field name="root_pid" />
                    <field name="father_pid" />
                    <field name="project" />
                    <field name="job" />
                    <field name="context" />
                    <field name="priority" />
                    <field name="type" />
                    <field name="origin" />
                    <field name="message" />
                    <field name="code" />
                </tree>
            </field>
        </record>

        <record id="view_talend_log_search" model="ir.ui.view">
            <field name="name">talend.log.search</field>
            <field name="model">talend.log</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Search">
                   <group col="6" colspan="4">
                        <field name="moment" />
						<field name="job" />
                    	<field name="message" />
                    </group>
               </search>
            </field>
        </record>

		<record id="action_talend_log" model="ir.actions.act_window">
            <field name="name">Talend Logs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">talend.log</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
		<menuitem action="action_talend_log" id="menu_talend_log" parent="base.menu_audit" sequence="30"/>

