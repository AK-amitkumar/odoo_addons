		<record id="view_server_action_log_tree" model="ir.ui.view">
			<field name="name">Server Action Logs</field>
			<field name="model">ir.actions.server.log</field>
			<field name="type">tree</field>
			<field name="inherit_id" ref="smile_ir_actions_extension.view_server_action_log_tree"/>
			<field name="arch" type="xml">
				<field name="end_date" position="after">
					<field name="talend_log_ids"/>
					<field name="talend_stats_ids"/>
					<field name="talend_meter_ids"/>
				</field>
			</field>
		</record>

    </data>
</openerp>